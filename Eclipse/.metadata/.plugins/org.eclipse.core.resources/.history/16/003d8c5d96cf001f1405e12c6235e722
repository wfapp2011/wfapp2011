import java.util.LinkedList;


public class Node {
	String name;
	LinkedList<Node> knoten;
	Node(String s){
		name = s;
		knoten = new LinkedList<Node>();
	}
	String getName(){
		return name;
	}
	void connect(Node r){
		knoten.add(r);
		r.knoten.add(this);
	}
	boolean disconnect(Node r){
		if (knoten.remove(r)){
			return r.knoten.remove(this);
		}
		return false;
	}
	
	
	public static void main(String[] args) {
		Node a = new Node("a");
		Node b = new Node("b");
		Node c = new Node("c");
		Node d = new Node("d");
		Node e = new Node("e");
		Node f = new Node("f");
		Node g = new Node("g");
		
		a.connect(b);
		a.connect(c);
		a.connect(c);
		d.connect(b);
		b.connect(e);
		e.connect(e);
		c.connect(f);
		f.connect(g);
		f.connect(g);
	}
}
