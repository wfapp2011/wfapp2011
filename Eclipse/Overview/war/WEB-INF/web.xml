<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Overview.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
  		<servlet-name>theStatisticProvider</servlet-name>
 		<servlet-class> de.uni_potsdam.hpi.wfapp2011.overview.server.StatisticProvider </servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>theStatisticProvider</servlet-name>
 		<url-pattern>/overview_module/statisticProvider</url-pattern>
	</servlet-mapping>
</web-app>