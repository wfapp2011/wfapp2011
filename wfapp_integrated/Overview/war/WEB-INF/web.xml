<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Overview.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
  		<servlet-name>theServerRequester</servlet-name>
 		<servlet-class> de.uni_potsdam.hpi.wfapp2011.overview.server.ServerRequester </servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>theServerRequester</servlet-name>
 		<url-pattern>/overview_module/serverRequester</url-pattern>
	</servlet-mapping>
</web-app>